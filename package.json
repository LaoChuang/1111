{
  "name": "manager-for-upyun",
  "version": "0.0.9",
  "author": {
    "name": "Michael Yin",
    "email": "layerssss@gmail.com"
  },
  "description": "又拍云资源管理器",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/layerssss/manager-for-upyun.git"
  },
  "scripts": {
    "app": "electron .",
    "release": "webpack && electron-builder build",
    "dist": "webpack && electron-builder build",
    "watch": "webpack --watch"
  },
  "main": "lib/app.js",
  "build": {
    "appId": "in.micy.managerforupyun",
    "productName": "Upyun Manager",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "protocols": [
      {
        "name": "UpYun URL",
        "schemes": [
          "upyun"
        ]
      }
    ],
    "linux": {
      "target": [
        "deb",
        "AppImage",
        "rpm"
      ],
      "category": "Development"
    },
    "win": {
      "target": [
        "squirrel"
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "electron-prompt": "^0.3.0",
    "electron-updater": "^4.1.2",
    "mkdirp": "^0.3.5",
    "request": "^2.34.0"
  },
  "devDependencies": {
    "ace-builds": "^1.2.8",
    "async": "^0.7.0",
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "coffee-loader": "^0.7.3",
    "coffee-script": "^1.12.7",
    "css-loader": "^0.28.4",
    "electron": "^6.0.5",
    "electron-builder": "^21.2.0",
    "electron-builder-squirrel-windows": "^21.2.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "font-awesome": "^4.7.0",
    "form-serializer": "^2.5.0",
    "jquery": "^2.2.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "md5": "^2.2.1",
    "messenger": "git+https://github.com/HubSpot/messenger.git#v1.4.1",
    "moment": "^2.6.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4"
  }
}
